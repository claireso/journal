{
  "name": "journal",
  "version": "1.0.0",
  "description": "A self-hosted website to show your photographs",
  "private": true,
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "next start",
