{
  "name": "journal",
  "version": "3.0.0",
  "description": "A self-hosted website to show your photographs",
  "private": true,
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:sw\" \"node server.js\"",
    "build": "npm run build:sw && next build",
    "build:sw": "NODE_ENV=production node scripts/build/serviceworker.mjs",
    "dev:sw": "WATCH=true node scripts/build/serviceworker.mjs",
    "start": "node .next/standalone/server.js",
    "preview": "next start",
    "start:express": "NODE_ENV=production COMPRESSION=enabled node server.js",
    "lint": "next lint",
    "lint:write": "next lint --fix",
    "prettier": "prettier --write \"./src/**/*.js\"",
    "test": "npm run test:web && npm run test:node",
    "test:web": "jest --coverage --passWithNoTests --verbose --config jest.config.js",
    "test:node": "jest --coverage --passWithNoTests --verbose --config jest.node.config.js",
    "sb": "storybook dev -p 6006",
    "db:reset": "babel-node scripts/db/reset.js",
    "db:seed": "babel-node scripts/db/seed.js",
    "prepare": "husky",
    "build-storybook": "storybook build",
    "e2e": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claireso/journal.git"
  },
  "author": "Claire Sosset",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/claireso/journal/issues"
  },
  "homepage": "https://github.com/claireso/journal#readme",
  "dependencies": {
    "@heroicons/react": "2.2.0",
    "@vanilla-extract/css": "1.18.0",
    "@vanilla-extract/recipes": "0.5.7",
    "better-auth": "1.4.18",
    "colorthief": "2.6.0",
    "date-fns": "4.1.0",
    "lodash": "4.17.21",
    "next": "15.4.10",
    "pg": "8.17.1",
    "pino": "10.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.34.5",
    "ulid": "3.0.2",
    "web-push": "3.6.7",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.28.6",
    "@babel/node": "7.28.6",
    "@changesets/cli": "2.29.8",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-links": "10.1.11",
    "@storybook/addon-webpack5-compiler-swc": "4.0.2",
    "@storybook/react-webpack5": "10.1.11",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.23",
    "@types/node": "22.10.5",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/web-push": "3.6.4",
    "@vanilla-extract/jest-transform": "1.1.20",
    "@vanilla-extract/next-plugin": "2.4.17",
    "@vanilla-extract/webpack-plugin": "2.3.25",
    "@vitest/mocker": "4.0.17",
    "babel-loader": "10.0.0",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-react-remove-properties": "0.3.1",
    "compression-webpack-plugin": "11.1.0",
    "concurrently": "9.2.1",
    "cypress": "9.5.4",
    "cypress-file-upload": "5.0.8",
    "eslint": "9.39.2",
    "eslint-config-next": "15.4.10",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-storybook": "10.1.11",
    "express": "4.21.2",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "lint-staged": "16.2.7",
    "mini-css-extract-plugin": "2.10.0",
    "pino-pretty": "13.1.3",
    "prettier": "3.8.0",
    "react-test-renderer": "19.1.0",
    "storybook": "10.1.11",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0"
  }
}
