{
  "name": "journal",
  "version": "2.6.1",
  "description": "A self-hosted website to show your photographs",
  "private": true,
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:sw\" \"node server.js\"",
    "build": "npm run build:sw && next build",
    "build:sw": "NODE_ENV=production node scripts/build/serviceworker.mjs",
    "dev:sw": "WATCH=true node scripts/build/serviceworker.mjs",
    "start": "node .next/standalone/server.js",
    "preview": "next start",
    "start:express": "NODE_ENV=production COMPRESSION=enabled node server.js",
    "lint": "next lint",
    "lint:write": "next lint --fix",
    "prettier": "prettier --write \"./src/**/*.js\"",
    "test": "npm run test:web && npm run test:node",
    "test:web": "jest --coverage --passWithNoTests --verbose --config jest.config.js",
    "test:node": "jest --coverage --passWithNoTests --verbose --config jest.node.config.js",
    "sb": "storybook dev -p 6006",
    "db:reset": "babel-node scripts/db/reset.js",
    "db:seed": "babel-node scripts/db/seed.js",
    "prepare": "husky",
    "build-storybook": "storybook build",
    "e2e": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claireso/journal.git"
  },
  "author": "Claire Sosset",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/claireso/journal/issues"
  },
  "homepage": "https://github.com/claireso/journal#readme",
  "dependencies": {
    "@heroicons/react": "2.2.0",
    "@vanilla-extract/css": "1.17.1",
    "@vanilla-extract/recipes": "0.5.5",
    "colorthief": "2.6.0",
    "date-fns": "4.1.0",
    "lodash": "4.17.21",
    "next": "15.2.4",
    "next-auth": "5.0.0-beta.25",
    "pg": "8.14.1",
    "pino": "9.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.33.5",
    "ulid": "2.3.0",
    "web-push": "3.6.7",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@babel/node": "7.26.0",
    "@changesets/cli": "2.28.1",
    "@storybook/addon-actions": "8.6.11",
    "@storybook/addon-essentials": "8.6.11",
    "@storybook/addon-links": "8.6.11",
    "@storybook/addon-styling-webpack": "1.0.1",
    "@storybook/addon-webpack5-compiler-swc": "3.0.0",
    "@storybook/react": "8.6.11",
    "@storybook/react-webpack5": "8.6.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.16",
    "@types/node": "22.10.5",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@types/web-push": "3.6.4",
    "@vanilla-extract/jest-transform": "1.1.14",
    "@vanilla-extract/next-plugin": "2.4.10",
    "@vanilla-extract/webpack-plugin": "2.3.18",
    "babel-loader": "10.0.0",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-react-remove-properties": "0.3.0",
    "compression-webpack-plugin": "11.1.0",
    "concurrently": "9.1.2",
    "cypress": "9.5.4",
    "cypress-file-upload": "5.0.8",
    "eslint": "9.23.0",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.5",
    "express": "4.21.2",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.5.0",
    "mini-css-extract-plugin": "2.9.2",
    "pino-pretty": "13.0.0",
    "prettier": "3.5.3",
    "react-test-renderer": "19.1.0",
    "storybook": "8.6.11",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0"
  }
}
